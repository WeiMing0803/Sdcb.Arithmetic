<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <SdcbGmpDlls>$(MSBuildThisFileDirectory)..\..\runtimes</SdcbGmpDlls>
  </PropertyGroup>
  <ItemGroup Condition="$(TargetFrameworkVersion.StartsWith('v4')) Or $(TargetFramework.StartsWith('net4'))">
    <Content Include="$(SdcbGmpDlls)\win-x64\native\gmp.dll">
      <Link>dll\x64\gmp.dll</Link>
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
    <Content Include="$(SdcbGmpDlls)\win-x64\native\gmp.pdb">
      <Link>dll\x64\gmp.pdb</Link>
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
  </ItemGroup>
</Project>